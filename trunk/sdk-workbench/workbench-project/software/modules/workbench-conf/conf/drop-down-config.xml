				<entry key="EA" value="Enterprise Architect"/>
				
				<entry key="mysql" value="MySQL"/>
				<entry key="oracle"  value="Oracle"/>
				
				<entry key="jboss" value="JBoss"/>
				<entry key="tomcat" value="Tomcat"/>
				
				<entry key="local" value="Local"/>
				<entry key="remote" value="Remote"/>
				
				<entry key="config19080"  value="Port Configuration 19080"/>
				<entry key="config19180"  value="Port Configuration 19180"/>
				<entry key="config19280"  value="Port Configuration 19280"/>
				<entry key="config19380"  value="Port Configuration 19380"/>
				<entry key="config19480"  value="Port Configuration 19480"/>
				<entry key="config19580"  value="Port Configuration 19580"/>
				<entry key="config19680"  value="Port Configuration 19680"/>
				<entry key="config29080"  value="Port Configuration 29080"/>
				<entry key="config39080"  value="Port Configuration 39080"/>
				<entry key="config49080"  value="Port Configuration 49080"/>
				<entry key="config29180"  value="Port Configuration 29180"/>
				<entry key="config39180"  value="Port Configuration 39180"/>
				<entry key="config49180"  value="Port Configuration 49180"/>
				<entry key="config59080"  value="Port Configuration 59080"/>
