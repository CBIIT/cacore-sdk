		</constructor-arg>
	</bean>
	
	<bean id="jbossPortConfigMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="Port Configuration 19080"  value="config19080"/>
				<entry key="Port Configuration 19180"  value="config19180"/>
				<entry key="Port Configuration 19280"  value="config19280"/>
				<entry key="Port Configuration 19380"  value="config19380"/>
				<entry key="Port Configuration 19480"  value="config19480"/>
				<entry key="Port Configuration 19580"  value="config19580"/>
				<entry key="Port Configuration 19680"  value="config19680"/>
				<entry key="Port Configuration 29080"  value="config29080"/>
				<entry key="Port Configuration 39080"  value="config39080"/>
				<entry key="Port Configuration 49080"  value="config49080"/>
				<entry key="Port Configuration 29180"  value="config29180"/>
				<entry key="Port Configuration 39180"  value="config39180"/>
				<entry key="Port Configuration 49180"  value="config49180"/>
				<entry key="Port Configuration 59080"  value="config59080"/>
				<entry key="Port Configuration 59180"  value="config59180"/>
