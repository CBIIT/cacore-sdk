	</bean>
	
	<bean id="caGridEnvironmentMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="Development 1.2" value="nci_dev-1.2"/>
				<entry key="Production 1.2" value="nci_prod-1.2"/>
				<entry key="QA 1.2" value="nci_qa-1.2"/>
				<entry key="Stage 1.2" value="nci_stage-1.2"/>
				<entry key="Training 1.2" value="training-1.2"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="remoteDeployEnvMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="Development" value="dev"/>
				<entry key="Other" value="other"/>
				<entry key="Production" value="prod"/>
				<entry key="QA" value="qa"/>
				<entry key="Stage" value="stage"/>
			</map>
		</constructor-arg>
	</bean>
		
	<bean id="clmDbTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="MySQL" value="mysql"/>
			</map>
		</constructor-arg>
	</bean>

	<bean id="dbTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="MySQL" value="mysql"/>
				<entry key="Oracle"  value="oracle"/>
			</map>
		</constructor-arg>
	</bean>
		
	<bean id="dbUrlMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="MySQL" value="jdbc:mysql://@HOSTNAME@:@PORT@/@SCHEMA@"/>
				<entry key="Oracle"  value="jdbc:oracle:thin:@@HOSTNAME@:@PORT@:@SCHEMA@"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="deployTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="" value=""/>
				<entry key="Local" value="local"/>
				<entry key="Remote"  value="remote"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="modelFileTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="ArgoUML" value="ARGO"/>
				<entry key="Enterprise Architect" value="EA"/>
			</map>
		</constructor-arg>
	</bean>
		
	<bean id="serverTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="JBoss" value="jboss"/>
				<entry key="Tomcat" value="tomcat"/>
			</map>
		</constructor-arg>
	</bean>

	
</beans>