<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<!-- When modifying one of the option maps, make sure to update the following map as well-->
	<bean id="valueToKeyMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				
				<entry key="nci_dev-1.2" value="Development 1.2"/>
				<entry key="nci_prod-1.2" value="Production 1.2"/>
				<entry key="nci_qa-1.2" value="QA 1.2"/>
				<entry key="nci_stage-1.2" value="Stage 1.2"/>
				<entry key="training-1.2" value="Training 1.2"/>
				
				<entry key="dev" value="Development"/>
				<entry key="prod" value="Production"/>
				<entry key="qa" value="QA"/>
				<entry key="stage" value="Stage"/>
				<entry key="other" value="Other"/>
				
				<entry key="ARGO" value="ArgoUML"/>
				<entry key="EA" value="Enterprise Architect"/>
				
				<entry key="mysql" value="MySQL"/>
				<entry key="oracle"  value="Oracle"/>
				
				<entry key="jboss" value="JBoss"/>
				<entry key="tomcat" value="Tomcat"/>
				
				<entry key="local" value="Local"/>
				<entry key="remote" value="Remote"/>
				
				<entry key="config19280"  value="Port Configuration 19280"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="jbossPortConfigMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="Configuration Port 19280"  value="config19280"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="caGridEnvironmentMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="Development 1.2" value="nci_dev-1.2"/>
				<entry key="Production 1.2" value="nci_prod-1.2"/>
				<entry key="QA 1.2" value="nci_qa-1.2"/>
				<entry key="Stage 1.2" value="nci_stage-1.2"/>
				<entry key="Training 1.2" value="training-1.2"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="remoteDeployEnvMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="Development" value="dev"/>
				<entry key="Other" value="other"/>
				<entry key="Production" value="prod"/>
				<entry key="QA" value="qa"/>
				<entry key="Stage" value="stage"/>
			</map>
		</constructor-arg>
	</bean>
		
	<bean id="clmDbTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="MySQL" value="mysql"/>
			</map>
		</constructor-arg>
	</bean>

	<bean id="dbTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="MySQL" value="mysql"/>
				<entry key="Oracle"  value="oracle"/>
			</map>
		</constructor-arg>
	</bean>
		
	<bean id="dbUrlMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="MySQL" value="jdbc:mysql://@HOSTNAME@:@PORT@/@SCHEMA@"/>
				<entry key="Oracle"  value="jdbc:oracle:thin:@@HOSTNAME@:@PORT@:@SCHEMA@"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="deployTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="" value=""/>
				<entry key="Local" value="local"/>
				<entry key="Remote"  value="remote"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="modelFileTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="ArgoUML" value="ARGO"/>
				<entry key="Enterprise Architect" value="EA"/>
			</map>
		</constructor-arg>
	</bean>
		
	<bean id="serverTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="JBoss" value="jboss"/>
				<entry key="Tomcat" value="tomcat"/>
			</map>
		</constructor-arg>
	</bean>

	
</beans>