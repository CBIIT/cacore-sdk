<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

	<!-- When modifying one of the option maps, make sure to update the following map as well-->
	<bean id="valueToKeyMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				
				<entry key="nci_prod-1.4" value="Production 1.4"/>
				<entry key="nci_qa-1.4" value="QA 1.4"/>
				<entry key="nci_stage-1.4" value="Stage 1.4"/>
				<entry key="training-1.4" value="Training 1.4"/>
				
				<entry key="dev" value="Development"/>
				<entry key="prod" value="Production"/>
				<entry key="qa" value="QA"/>
				<entry key="stage" value="Stage"/>
				<entry key="other" value="Other"/>
				
				<entry key="ARGO" value="ArgoUML"/>
				<entry key="EA" value="Enterprise Architect"/>
				
				<entry key="mysql" value="MySQL"/>
				<entry key="oracle"  value="Oracle"/>
				<entry key="postgresql"  value="PostgreSQL"/>
				
				<entry key="jboss" value="JBoss"/>
				<entry key="tomcat" value="Tomcat"/>
				
				<entry key="local" value="Local"/>
				<entry key="remote" value="Remote"/>
				
				<entry key="ports-29080"  value="ports-29080"/>
				<entry key="ports-29180"  value="ports-29180"/>
				<entry key="ports-29280"  value="ports-29280"/>
				<entry key="ports-29380"  value="ports-29380"/>
				<entry key="ports-29480"  value="ports-29480"/>
				<entry key="ports-29580"  value="ports-29580"/>
				<entry key="ports-29680"  value="ports-29680"/>
				<entry key="ports-29780"  value="ports-29780"/>
				<entry key="ports-29880"  value="ports-29880"/>
				<entry key="ports-29980"  value="ports-29980"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="jbossPortNameMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="ports-29080"  value="ports-29080"/>
				<entry key="ports-29180"  value="ports-29180"/>
				<entry key="ports-29280"  value="ports-29280"/>
				<entry key="ports-29380"  value="ports-29380"/>
				<entry key="ports-29480"  value="ports-29480"/>
				<entry key="ports-29580"  value="ports-29580"/>
				<entry key="ports-29680"  value="ports-29680"/>
				<entry key="ports-29780"  value="ports-29780"/>
				<entry key="ports-29880"  value="ports-29880"/>
				<entry key="ports-29980"  value="ports-29980"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="caGridEnvironmentMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="Production 1.4" value="nci_prod-1.4"/>
				<entry key="QA 1.4" value="nci_qa-1.4"/>
				<entry key="Stage 1.4" value="nci_stage-1.4"/>
				<entry key="Training 1.4" value="training-1.4"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="remoteDeployEnvMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="Development" value="dev"/>
				<entry key="Other" value="other"/>
				<entry key="Production" value="prod"/>
				<entry key="QA" value="qa"/>
				<entry key="Stage" value="stage"/>
			</map>
		</constructor-arg>
	</bean>

	<bean id="dbTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="MySQL" value="mysql"/>
				<entry key="Oracle" value="oracle"/>
				<entry key="PostgreSQL" value="postgresql"/>
			</map>
		</constructor-arg>
	</bean>
		
	<bean id="dbUrlMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="MySQL" value="jdbc:mysql://@HOSTNAME@:@PORT@/@SCHEMA@"/>
				<entry key="Oracle"  value="jdbc:oracle:thin:@@HOSTNAME@:@PORT@:@SCHEMA@"/>
				<entry key="PostgreSQL" value="jdbc:postgresql://@HOSTNAME@:@PORT@/@SCHEMA@"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="deployTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="" value=""/>
				<entry key="Local" value="local"/>
				<entry key="Remote"  value="remote"/>
			</map>
		</constructor-arg>
	</bean>
	
	<bean id="modelFileTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key=""  value=""/>
				<entry key="ArgoUML" value="ARGO"/>
				<entry key="Enterprise Architect" value="EA"/>
			</map>
		</constructor-arg>
	</bean>
		
	<bean id="serverTypeMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="JBoss" value="jboss"/>
				<entry key="Tomcat" value="tomcat"/>
			</map>
		</constructor-arg>
	</bean>

	
</beans>